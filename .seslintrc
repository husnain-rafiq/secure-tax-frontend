{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",

  ],
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 100,
        "singleQuote": true,
        "trailingComma": "es5",
        "indentStyle": "tab",
        "tabWidth": 2,
        "useTabs": true,
        "endOfLine": "auto"
      }
    ],
    "prefer-const": "error",
    "react/prop-types": 0,
    "no-unused-vars": "error",
    "no-undef": "error",
    "no-console": "warn",
    "spaced-comment": "off"
  }
}